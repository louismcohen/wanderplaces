        "googleMapsApiKey": "AIzaSyBVWoxT7UAn5cgvXUjLGOSmnzkRPUS5RI8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
